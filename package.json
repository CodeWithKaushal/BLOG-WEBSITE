{
  "name": "blog-website",
  "version": "1.0.0",
  "description": "A full-stack blog website built with the MERN stack",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon api/index.js",
    "start": "node api/index.js",
    "client": "cd client && npm run dev",
    "dev-all": "concurrently \"npm run dev\" \"npm run client\"",
    "build": "npm install && cd client && npm install && npm run build"
  },
  "keywords": [
    "blog",
    "mern",
    "react",
    "node",
    "express",
    "mongodb"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "mongoose": "^7.4.3",
    "nodemon": "^3.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "fs-extra": "^11.2.0"
  }
}
